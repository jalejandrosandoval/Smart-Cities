{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/astro/*": [
        "src/components/astro/*"
      ],
      "@/preact/*": [
        "src/components/preact/*"
      ],
      "@/pages/*": [
        "src/components/Pages/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}